# Generated by Chef for <%= node[:fqdn] %>
global {
    minor-count  5;
    dialog-refresh 1;
    usage-count no;
}
common {
  protocol C;

  startup {
    wfc-timeout 1;
    outdated-wfc-timeout 1;
    degr-wfc-timeout 1;
  }

  disk {
    no-disk-barrier;
    no-disk-flushes;
  }

  net {
    max-buffers 8000;
    max-epoch-size 8000;
    sndbuf-size 512k;
    unplug-watermark 16;
  }

  syncer {
    rate 128M;
  }
}
